{
  "name": "react-path-tracker",
  "version": "0.1.0",
  "description": "Easily track traveled path on highly dynamic React trees.",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -Rf lib",
    "build": "npm run clean && babel src -d lib",
    "lint": "eslint src tests",
    "test": "jest",
    "prepush": "npm run lint && npm run test",
    "prepublish": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "react",
    "path",
    "recursivity"
  ],
  "author": "Lucas Constantino Silva <lucasconstantinosilva@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-taller": "^0.1.1",
    "babel-register": "^6.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-taller": "^1.1.4",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^19.0.2",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "recompose": "^0.26.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasconstantino/react-path-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/lucasconstantino/react-path-tracker/issues"
  },
  "homepage": "https://github.com/lucasconstantino/react-path-tracker#readme",
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/enzyme.setup.js"
    ],
    "moduleNameMapper": {
      "react-path-tracker": "<rootDir>/src"
    }
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
